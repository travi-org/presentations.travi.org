{
  "name": "presentations.travi.org",
  "description": "Site to list presentations I've given",
  "license": "MIT",
  "private": true,
  "repository": "travi/presentations.travi.org",
  "bugs": "https://github.com/travi/presentations.travi.org/issues",
  "homepage": "https://github.com/travi/presentations.travi.org#readme",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "start": "run-s develop",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "test": "npm-run-all --print-label --parallel lint:*",
    "deploy:netlify": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./public --prod"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "1.6.18",
    "babel-register": "6.26.0",
    "ban-sensitive-files": "1.9.2",
    "commitlint-config-travi": "1.2.31",
    "cz-conventional-changelog": "2.1.0",
    "husky": "1.1.2",
    "npm-run-all": "4.1.3",
    "remark-cli": "6.0.0",
    "remark-preset-lint-travi": "1.1.0",
    "travis-lint": "1.0.0"
  },
  "dependencies": {
    "gatsby": "2.0.31",
    "gatsby-image": "2.0.17",
    "gatsby-plugin-sharp": "2.0.8",
    "gatsby-source-filesystem": "2.0.5",
    "gatsby-transformer-screenshot": "2.0.4",
    "gatsby-transformer-sharp": "2.1.5",
    "gatsby-transformer-yaml": "2.1.3",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-dom": "16.6.0"
  }
}
