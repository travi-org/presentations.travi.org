{
  "name": "presentations.travi.org",
  "description": "Site to list presentations I've given",
  "license": "MIT",
  "private": true,
  "repository": "travi/presentations.travi.org",
  "bugs": "https://github.com/travi/presentations.travi.org/issues",
  "homepage": "https://travi-presentations.netlify.com/",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "start": "run-s develop",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label --parallel lint:* build",
    "predeploy:netlify": "run-s build",
    "deploy:netlify": "netlify deploy --dir=./public --prod",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "10.2.1",
    "@travi/eslint-config-travi": "1.8.8",
    "babel-eslint": "10.1.0",
    "babel-preset-gatsby": "0.12.3",
    "babel-register": "6.26.0",
    "ban-sensitive-files": "1.9.16",
    "commitlint-config-travi": "1.4.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "netlify-cli": "8.1.4",
    "npm-run-all": "4.1.5",
    "remark-cli": "10.0.1",
    "remark-preset-lint-travi": "2.0.9"
  },
  "dependencies": {
    "@emotion/core": "10.3.1",
    "@material-ui/core": "4.12.3",
    "@travi/brand": "1.0.0",
    "@travi/travi.org-theme-components": "1.2.0",
    "gatsby": "2.32.13",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-emotion": "4.5.0",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-material-ui": "2.1.10",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-sharp": "2.14.4",
    "gatsby-plugin-webmention": "0.2.0",
    "gatsby-source-filesystem": "2.11.1",
    "gatsby-transformer-screenshot": "2.10.0",
    "gatsby-transformer-sharp": "2.12.1",
    "gatsby-transformer-yaml": "2.11.0",
    "polished": "4.1.3",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-emotion": "10.0.0",
    "react-helmet": "6.1.0"
  }
}
